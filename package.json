{
  "name": "server",
  "version": "1.0.0",
  "main": "src/server.js",
  "private": true,
  "scripts": {
    "start": "node -r ./node_modules/dotenv/config dist/server.js",
    "python": "pip3 install numpy pymongo pandas datetime",
    "dev": "npm run python & nodemon --delay 2 --exec babel-node src/server.js -r ./node_modules/dotenv/config",
    "debug": "nodemon --exec babel-node --inspect src/server.js -r ./node_modules/dotenv/config",
    "build": "babel src --out-dir dist",
    "test": "NODE_ENV=test babel-node src/server.js -r ./node_modules/dotenv/config",
    "lint": "eslint ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "app-root-path": "^2.1.0",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-ws": "^4.0.0",
    "firebase-admin": "^7.0.0",
    "handlebars": "^4.5.3",
    "http-errors": "^1.7.2",
    "jade": "^1.11.0",
    "mongodb": "^3.2.3",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.3.0",
    "python-shell": "^1.0.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0"
  }
}
